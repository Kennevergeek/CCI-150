Write a method to replace all spaces in a string with â€˜%20'.

vector<string> replaceSpace(vector<string> &res, string s)
{
	int len = s.length();
	int p = 0;

	for (int i = 0; i<len; i++)
	{
		if (s[i] != ' ')
		{
			res[p] = s[i];
		}
		else
		{
			res.push_back("%20");
			p += 3;
		}
	}
	return res;
}
